{
	"version": 1,
	"name": "scaffold",
	"config": [
		"# The app name can be referenced from within your app",
		"app.name = %%namespace%%",
		"",
		"# Requests for paths starting with these comma-separated directories will be",
		"# found in the static/ directory of this project.",
		"static.directories = static",
		"",
		"# Show log items at or above the value. (debug, info, warn, error)",
		"app.log_level = debug",
		"",
		"# Display the Powered-By HTTP header",
		"app.show_powered_by = yes",
		"",
		"# Uncomment to define a session.",
		"# session.store = StorableFile",
		"# session.expires = 3600",
		"# storable.file.path = /tmp",
		"",
		"# If using the SCGI engine, you can configure the maximum number of threads or",
		"# the port to listen on.",
		"scgi.port = 3200",
		"scgi.threads = 10"
	],
	"creates": [
		"meson.build",
		"config",
		"config/build.conf",
		"config/log4vala.conf",
		"config/plugins.conf",
		"src",
		"src/meson.build",
		"src/Application.vala",
		"src/Main.vala",
		"src/Controller",
		"src/Controller/Root.vala",
		"src/Form",
		"src/Model",
		"src/View",
		"src/View/Template",
		"src/View/Template/Root",
		"src/View/Template/Root/index.vtmpl",
		"static",
		"test",
		"test/meson.build",
		"test/InitializeTests.vala",
		"test/ApplicationTest.vala"
	]
}
