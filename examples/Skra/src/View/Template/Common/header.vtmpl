@parameters( string node, bool exists, string subtitle )
<html>
<head>
	<title>@{Ambition.Config.lookup_with_default( "wiki.name", "Skra Wiki" )} - @{subtitle}</title>
	@{state.request.cache["head_markdown"]}
</head>
<body>
@{state.request.cache["header_markdown"].replace( "|% loginbox %|", ( new Skra.View.Template.Common.loginbox() ).to_string_with_state(state) ).replace( "|% editbox %|", ( new Skra.View.Template.Common.editbox( exists, node ) ).to_string_with_state(state) ).replace( "|% breadcrumbs %|", ( new Skra.View.Template.Common.breadcrumbs( node ) ).to_string_with_state(state) )}